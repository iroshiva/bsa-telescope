{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "telescope-blocks/hero-section",
	"version": "0.1.0",
	"title": "En-tête de page",
	"category": "telescope",
	"icon": "cover-image",
	"description": "En-tête de page",
	"supports": {
		"html": false,
		"jsx": true,
		"align": ["full", "wide"],
		"layout": {
			"default": {
				"type": "constrained",
				"justifyContent": "center",
				"orientation": "vertical"
			},
			"allowVerticalAlignment": false
		}
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		}
	},
	"keywords": ["hero", "en-tête"],
	"textdomain": "telescope-blocks",
	"example": {
		"attributes": {
			"content": "Hello world"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
