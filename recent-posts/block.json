{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "telescope-blocks/recent-posts",
	"version": "0.1.0",
	"title": "Articles Récents",
	"category": "telescope",
	"icon": "smiley",
	"description": "Récupération des 3 derniers articles",
	"supports": {
		"html": false,
		"jsx": true,
		"align": ["full", "wide"]
	},
	"keywords": ["articles", "posts"],
	"textdomain": "telescope-blocks",
	"attributes": {
		"SelectedPostType": {
			"type": "string",
			"default": "post"
		},
		"PostType": {
			"type": "object"
		},
		"postperpage": {
			"type": "number",
			"default": 5
		},
		"Order": {
			"type": "string",
			"default": "ASC"
		},
		"postdate": {
			"type": "boolean",
			"default": false
		},
		"postcontent": {
			"type": "boolean",
			"default": false
		},
		"imagesize": {
			"type": "string",
			"default": "large"
		}
	},
	"example": {
		"attributes": {
			"content": "Hello world"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
