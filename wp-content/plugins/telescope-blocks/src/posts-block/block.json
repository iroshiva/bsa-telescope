{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "telescope-blocks/posts-block",
	"version": "0.1.0",
	"title": "Articles",
	"category": "telescope",
	"icon": "smiley",
	"description": "Récupération des 3 derniers articles",
	"supports": {
		"html": false,
		"jsx": true
	},
	"keywords": ["articles", "posts"],
	"textdomain": "telescope-blocks",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"example": {
		"attributes": {
			"content": "Hello world"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
