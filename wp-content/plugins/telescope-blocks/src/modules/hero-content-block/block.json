{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "telescope-blocks/hero-content-block",
	"version": "0.1.0",
	"title": "Contenu d'en-tête",
	"category": "telescope",
	"icon": "smiley",
	"description": "Permet de remplir le contenu d'un bloc d'en-tête avec un titre, paragraphe et bouton",
	"supports": {
		"html": false,
		"jsx": true
	},
	"keywords": ["contenu", "en-tête"],
	"textdomain": "telescope-blocks",
	"example": {
		"attributes": {
			"content": "Hello world"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
