{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "telescope-blocks/half-section-content",
	"version": "0.1.0",
	"title": "Contenu de la section half",
	"category": "telescope",
	"icon": "editor-paste-text",
	"description": "Permet de remplir le contenu d'une section Half",
	"supports": {
		"html": false,
		"jsx": true,
		"alignVertical": true,
		"alignJustify": true,
		"layout": {
			"default": {
				"type": "flex",
				"justifyContent": "center",
				"orientation": "vertical"
			},
			"allowVerticalAlignment": false
		}
	},
	"keywords": ["contenu", "half"],
	"textdomain": "telescope-blocks",
	"example": {
		"attributes": {
			"content": "Hello world"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
